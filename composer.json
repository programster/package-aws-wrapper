{
    "name": "programster/aws-wrapper",
    "type": "library",
    "description": "A package to make it easier to interface with the AWS API.",
    "keywords": ["AWS"],
    "homepage": "https://github.com/programster/package-aws-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "sdpagent@gmail.com",
            "homepage": "https://blog.programster.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "irap/core-libs": "1.1.*",
        "aws/aws-sdk-php" : "3.24.*",
        "ext-libxml" : "*",
        "ext-simplexml" : "*"
    },
    "autoload": {
        "psr-4": {
            "Programster\\AwsWrapper\\": "",
            "Programster\\AwsWrapper\\Enums\\": "enums/",
            "Programster\\AwsWrapper\\Objects\\": "objects/",
            "Programster\\AwsWrapper\\Requests\\": "requests/",
            "Programster\\AwsWrapper\\Responses\\": "responses/",
            "Programster\\AwsWrapper\\S3\\": "S3/",
            "Programster\\AwsWrapper\\Ec2\\": "Ec2/"
        }
    }
}
